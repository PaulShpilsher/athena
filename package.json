{
  "name": "athena",
  "version": "0.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json src/**/*.ts --fix",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/server.ts",
    "start": "cross-env NODE_ENV=production forever start -a --uid 'node-athena' -v -c ts-node src/server.ts",
    "stop": "cross-env NODE_ENV=production forever stop 'node-athena'"
  },
  "author": "Paul Shpilsher",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.44",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/log4js": "^2.3.5",
    "@types/mongoose": "^5.5.21",
    "@types/node": "^12.11.1",
    "@types/redis": "^2.8.14",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.21.2",
    "http-status": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "log4js": "^6.7.1",
    "mongodb": "^3.3.3",
    "mongoose": "^5.13.15",
    "nodemon": "^2.0.20",
    "redis": "^4.6.4"
  }
}
