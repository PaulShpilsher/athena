{
  "name": "athena",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json src/**/*.ts --fix",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/index.ts",
    "start": "cross-env NODE_ENV=production forever start -a --uid 'node-athena' -v -c ts-node src/index.ts",
    "stop": "cross-env NODE_ENV=production forever stop 'node-athena'"
  },
  "author": "Paul Shpilsher",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/log4js": "^2.3.5",
    "@types/mongoose": "^5.5.21",
    "@types/node": "^12.11.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.3.5",
    "bcrypt": "^3.0.6",
    "class-validator": "^0.10.2",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^5.2.2",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.5",
    "nodemon": "^1.19.4"
  }
}
